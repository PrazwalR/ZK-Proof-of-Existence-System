# Privy Authentication (required)
# This is your Privy App ID - get it from https://dashboard.privy.io
# NOTE: App IDs are not secrets - they're meant to be public
VITE_PRIVY_APP_ID=your_privy_app_id_here

# Pinata IPFS (optional - only needed for certificate pinning to IPFS)
# Get a free JWT at https://app.pinata.cloud/developers/api-keys
# Leave empty if you don't need IPFS functionality
VITE_PINATA_JWT=
